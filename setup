term.clear()
term.setCursorPos(1,1)
print("Give your computer a label: ")
os.setComputerLabel(read())

local loginfo = fs.open("loginfo", "w")

print("Username?")
loginfo.writeLine(read())
print("Password?")
loginfo.writeLine(read("*"))

loginfo.close()

local stup = fs.open("startup", "w")
stup.write("shell.run("login")

shell.run("login")